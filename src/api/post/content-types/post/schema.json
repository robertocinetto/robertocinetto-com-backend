{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": false,
      "maxLength": 255
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Tutorials",
        "Tips",
        "News"
      ],
      "required": true
    },
    "Summary": {
      "type": "text",
      "required": true,
      "maxLength": 225
    },
    "FeaturedImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "default-seo.default-seo"
    },
    "Content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
